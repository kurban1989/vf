<div class="Content-table__header">
        <h1 class="js-headerCab">Заказы</h1>
        <div>
            &nbsp;
            <button class="Content-table__add-btn Button--danger js-btnAddProd" title="Удалить заказ из базы данных" data-src="deleteProd">
                <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 22 20" enable-background="new 0 0 22 20" xml:space="preserve" style="width: 20px;height: 16px;"><path fill="#fff" d="M18,1h-5c0-0.55-0.45-1-1-1h-2C9.45,0,9,0.45,9,1H4C3.45,1,3,1.45,3,2s0.45,1,1,1h14c0.55,0,1-0.45,1-1 S18.55,1,18,1z"></path><path fill="#fff" d="M16,4H6C4.9,4,4,4.9,4,6v12c0,1.1,0.9,2,2,2h10c1.1,0,2-0.9,2-2V6C18,4.9,17.1,4,16,4z M8,17 c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1s1,0.45,1,1V17z M12,17c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1 s1,0.45,1,1V17z M16,17c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1s1,0.45,1,1V17z"></path></svg>
            </button>
        </div>
    </div>
    <div class="Content-table__main">
        <div class="Content-table__wrapper-table js-scrollContent simplebar">
            <div class="Content-table__table">
                <div class="Content-table__panel-head">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" height="32px" id="Layer_1" style="enable-background:new 0 0 32 32;" version="1.1" viewBox="0 0 32 32" width="32px" xml:space="preserve"><path d="M4,10h24c1.104,0,2-0.896,2-2s-0.896-2-2-2H4C2.896,6,2,6.896,2,8S2.896,10,4,10z M28,14H4c-1.104,0-2,0.896-2,2  s0.896,2,2,2h24c1.104,0,2-0.896,2-2S29.104,14,28,14z M28,22H4c-1.104,0-2,0.896-2,2s0.896,2,2,2h24c1.104,0,2-0.896,2-2  S29.104,22,28,22z"/></svg>
                    <h3>Заказы</h3>
                </div>
                <div class="Content-table__row">
                    <div class="Content-table__cell">
                        <input type="checkbox" class="Content-table__filter--form-control checkbox" onclick="dom.query('input[name*=\'selected\']').prop('checked', this.checked);">
                    </div>
                    <div class="Content-table__cell"><p class="title">№ заказа</p></div>
                    <div class="Content-table__cell"><p class="title">Дата заказа</p></div>
                    <div class="Content-table__cell"><p class="title">Имя/Фамилия заказчика</p></div>
                    <div class="Content-table__cell"><p class="title">Адрес доставки</p></div>
                    <div class="Content-table__cell"><p class="title">Сумма заказа</p></div>
                    <div class="Content-table__cell"><p class="title">Сумма доставки заказа</p></div>
                    <div class="Content-table__cell"><p class="title">Статус Заказа</p></div>
                    <div class="Content-table__cell"><p class="title">Статус оплаты</p></div>
                    <div class="Content-table__cell"><p class="title">Действия</p></div>
                </div>
                <% table.forEach(function(item){ %>
                    <div class="Content-table__row">
                        <div class="Content-table__cell">
                            <input type="checkbox" class="Content-table__filter--form-control checkbox" name="selected[]" data-product_id="<%=item.id%>">
                        </div>

                        <div class="Content-table__cell"><p class="text"><%- item.id %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.date %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.checkoutFirst_name %> <br> <%- item.checkoutLast_name %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.checkoutRegion %>, г. <%- item.checkoutCity %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.sum_checkout %> руб.</p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.cost_delivery %> руб.</p></div>
                        <div class="Content-table__cell"><p class="text">
                            <% if (item.new == 1) { %> <b class="pay-yes">Новый</b> <% } else { %> <b class="pay-orange">Обработанный</b> <% } %>
                        </p></div>
                        <div class="Content-table__cell"><p class="text">
                            <% if (item.status_pay == 1) { %> <b class="pay-yes">Оплачено</b> <% } else { %> <b class="pay-no">Оплаты не было</b> <% } %>
                        </p></div>
                        <div class="Content-table__cell">
                            <button class="Content-table__add-btn js-btnAddProd" title="Подробный просмотр Заказа" style="font-size:14px;" data-src="setNewWindowAdd" data-product_id="<%= item.user_token %>">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 30 30" style="width:20px;height:20px;margin-top:4px;
"><path d="M 15 5 C 6.081703 5 0.32098813 14.21118 0.21679688 14.378906 A 1 1 0 0 0 0 15 A 1 1 0 0 0 0.16210938 15.544922 A 1 1 0 0 0 0.16601562 15.550781 C 0.18320928 15.586261 5.0188313 25 15 25 C 24.938822 25 29.767326 15.678741 29.826172 15.564453 A 1 1 0 0 0 29.837891 15.544922 A 1 1 0 0 0 30 15 A 1 1 0 0 0 29.785156 14.380859 A 1 1 0 0 0 29.783203 14.378906 C 29.679012 14.21118 23.918297 5 15 5 z M 15 8 C 18.866 8 22 11.134 22 15 C 22 18.866 18.866 22 15 22 C 11.134 22 8 18.866 8 15 C 8 11.134 11.134 8 15 8 z M 15 12 A 3 3 0 0 0 12 15 A 3 3 0 0 0 15 18 A 3 3 0 0 0 18 15 A 3 3 0 0 0 15 12 z"></path></svg>
                            </button>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
</div>