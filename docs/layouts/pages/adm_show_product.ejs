<div class="Content-table__header">
        <h1 class="js-headerCab">Товары</h1>
        <div>
            <button class="Content-table__add-btn js-btnAddProd" title="Добавить товар на сайт" data-src="setNewWindowAdd">&nbsp;+&nbsp;</button>
            &nbsp;&nbsp;
            <button class="Content-table__add-btn Button--danger js-btnAddProd" title="Удалить товар" data-src="deleteProd">
                <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 22 20" enable-background="new 0 0 22 20" xml:space="preserve" style="width: 20px;height: 16px;"><path fill="#fff" d="M18,1h-5c0-0.55-0.45-1-1-1h-2C9.45,0,9,0.45,9,1H4C3.45,1,3,1.45,3,2s0.45,1,1,1h14c0.55,0,1-0.45,1-1 S18.55,1,18,1z"></path><path fill="#fff" d="M16,4H6C4.9,4,4,4.9,4,6v12c0,1.1,0.9,2,2,2h10c1.1,0,2-0.9,2-2V6C18,4.9,17.1,4,16,4z M8,17 c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1s1,0.45,1,1V17z M12,17c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1 s1,0.45,1,1V17z M16,17c0,0.55-0.45,1-1,1s-1-0.45-1-1V7c0-0.55,0.45-1,1-1s1,0.45,1,1V17z"></path></svg>
            </button>
        </div>
    </div>
    <div class="Content-table__main">
        <div class="Content-table__wrapper-table js-scrollContent simplebar">
            <div class="Content-table__table">
                <div class="Content-table__panel-head">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" height="32px" id="Layer_1" style="enable-background:new 0 0 32 32;" version="1.1" viewBox="0 0 32 32" width="32px" xml:space="preserve"><path d="M4,10h24c1.104,0,2-0.896,2-2s-0.896-2-2-2H4C2.896,6,2,6.896,2,8S2.896,10,4,10z M28,14H4c-1.104,0-2,0.896-2,2  s0.896,2,2,2h24c1.104,0,2-0.896,2-2S29.104,14,28,14z M28,22H4c-1.104,0-2,0.896-2,2s0.896,2,2,2h24c1.104,0,2-0.896,2-2  S29.104,22,28,22z"/></svg>
                    <h3>Товары</h3>
                </div>
                <div class="Content-table__row" id="header_row_table">
                    <div class="Content-table__cell">
                        <input type="checkbox" class="Content-table__filter--form-control checkbox" onclick="dom.query('input[name*=\'selected\']').prop('checked', this.checked);">
                    </div>
                    <div class="Content-table__cell"><p class="title">Изображение</p></div>
                    <div class="Content-table__cell"><p class="title">Наименование товара</p></div>
                    <div class="Content-table__cell"><p class="title">Брэнд</p></div>
                    <div class="Content-table__cell"><p class="title">Категория</p></div>
                    <div class="Content-table__cell"><p class="title">Цена на сайте</p></div>
                    <div class="Content-table__cell"><p class="title">Видимость на сайте</p></div>
                    <div class="Content-table__cell"><p class="title">Описание</p></div>
                    <div class="Content-table__cell"><p class="title">Количество</p></div>
                    <div class="Content-table__cell"><p class="title">Действия</p></div>
                </div>
                <% table.forEach(function(item){ %>
                    <% var imgPreview; %>
                    <div class="Content-table__row js-removeRow">
                        <div class="Content-table__cell">
                            <input type="checkbox" class="Content-table__filter--form-control checkbox" name="selected[]" data-product_id="<%=item.id%>">
                        </div>
                        <% if (item.images && item.images != '' && item.images.length > 5) {imgPreview = item.images.split(','); %>
                        <div class="Content-table__cell"><img class="preview" data-src="<%- imgPreview[0] %>" alt=""></div>
                        <%} else {%>
                        <div class="Content-table__cell"><img class="preview" data-src="/img/svg/no-photo.svg" alt=""></div>
                        <% } %>
                        <div class="Content-table__cell"><p class="text"><%- item.title %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.vendor %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.category %></p></div>
                        <div class="Content-table__cell"><p class="text"><span class="price"><%= item.price %> </span><br> <span class="old-price"><%= item.old_price %> </span></p></div>
                        <div class="Content-table__cell"><p class="text"><% if (item.visible == 1) {%> Да <%} else {%> Нет <% } %></p></div>
                        <div class="Content-table__cell"><p class="text"><%- item.discription %></p></div>
                        <div class="Content-table__cell"><p class="text"><%= item.stock_count %></p></div>
                        <div class="Content-table__cell">
                           <button class="Content-table__add-btn js-btnAddProd" title="Редактирование товара" style="font-size: 14px;" data-src="setNewWindowAdd" data-product_id="<%=item.id%>">
                                <svg class="pensal" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 22 20" enable-background="new 0 0 22 20" xml:space="preserve"><g><path fill="#fff" d="M17.73,3.27c-1.48-1.48-2.591-1.258-2.591-1.258L9.958,7.193l-5.922,5.922L3,18l4.885-1.036l5.922-5.922 l5.182-5.181C18.988,5.861,19.21,4.75,17.73,3.27z M7.594,16.377l-1.665,0.359c-0.161-0.301-0.354-0.601-0.709-0.956 c-0.354-0.354-0.655-0.547-0.956-0.709l0.359-1.665l0.481-0.482c0,0,0.906,0.018,1.929,1.042c1.023,1.023,1.042,1.93,1.042,1.93 L7.594,16.377z"></path></g></svg>
                            </button>
                        </div>
                    </div>
                <% }) %>
            </div>
        </div>
        <div class="Content-table__filter">
            <div class="Content-table__filter--header">
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Capa_1" x="0px" y="0px" width="533.333px" height="533.333px" viewBox="0 0 533.333 533.333" style="enable-background:new 0 0 533.333 533.333;" xml:space="preserve">
                    <path d="M266.667,0C119.391,0,0,37.309,0,83.333v50l200,200V500c0,18.409,29.847,33.333,66.667,33.333   c36.818,0,66.667-14.924,66.667-33.333V333.333l200-200v-50C533.333,37.309,413.942,0,266.667,0z M49.158,72.304   c12.471-7.114,29.991-13.865,50.666-19.525c45.805-12.54,105.056-19.446,166.843-19.446c61.786,0,121.039,6.906,166.843,19.446   c20.675,5.66,38.194,12.411,50.666,19.525c8.226,4.69,12.669,8.659,14.738,11.029c-2.069,2.371-6.514,6.337-14.738,11.029   c-12.472,7.114-29.991,13.865-50.666,19.525c-45.803,12.54-105.056,19.446-166.843,19.446c-61.787,0-121.038-6.906-166.843-19.446   c-20.675-5.661-38.195-12.412-50.666-19.525c-8.225-4.691-12.669-8.658-14.739-11.029C36.489,80.963,40.933,76.996,49.158,72.304z"/>
                </svg>
                <h3>Фильтры</h3>
            </div>
            <div class="Content-table__filter--form">
                <form action="" id="filter_group">
                    <div class="Content-table__filter--form-group">
                        <label for="input-title">Наименование товара</label>
                        <input type="text" name="filter-title" value="" placeholder="Наименование товара" id="input-title" class="Content-table__filter--form-control js-keyPopup" autocomplete="off">
                        <ul class="Content-table__dropdown-menu js-categoryPopup"></ul>
                    </div>
                    <div class="Content-table__filter--form-group">
                        <label for="input-category">Категория товара</label>
                        <input type="text" name="filter-category" value="" placeholder="Категория товара" id="input-category" class="Content-table__filter--form-control js-keyPopup" autocomplete="off">
                        <ul class="Content-table__dropdown-menu js-categoryPopup"></ul>
                    </div>
                    <div class="Content-table__filter--form-group">
                        <label for="input-price">Цена</label>
                        <input type="number" name="filter-price" value="" placeholder="Цена" id="input-price" class="Content-table__filter--form-control" autocomplete="off">
                    </div>
                    <div class="Content-table__filter--form-group">
                        <label for="input-stock_count">Количество на складе</label>
                        <input type="number" name="filter-stock_count" value="" placeholder="Количество на складе" id="input-stock_count" class="Content-table__filter--form-control" autocomplete="off">
                    </div>
                    <div class="Content-table__filter--form-group">
                        <label for="input-visible">Только видимые на сайте</label>
                        <input type="checkbox" name="filter-visible" id="input-visible" class="Content-table__filter--form-control checkbox">
                    </div>
                    <div class="Content-table__filter--form-group right-text">
                        <button type="button" id="button-filter">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1" id="Capa_1" x="0px" y="0px" width="533.333px" height="533.333px" viewBox="0 0 533.333 533.333" style="enable-background:new 0 0 533.333 533.333;" xml:space="preserve">
                                <path d="M266.667,0C119.391,0,0,37.309,0,83.333v50l200,200V500c0,18.409,29.847,33.333,66.667,33.333   c36.818,0,66.667-14.924,66.667-33.333V333.333l200-200v-50C533.333,37.309,413.942,0,266.667,0z M49.158,72.304   c12.471-7.114,29.991-13.865,50.666-19.525c45.805-12.54,105.056-19.446,166.843-19.446c61.786,0,121.039,6.906,166.843,19.446   c20.675,5.66,38.194,12.411,50.666,19.525c8.226,4.69,12.669,8.659,14.738,11.029c-2.069,2.371-6.514,6.337-14.738,11.029   c-12.472,7.114-29.991,13.865-50.666,19.525c-45.803,12.54-105.056,19.446-166.843,19.446c-61.787,0-121.038-6.906-166.843-19.446   c-20.675-5.661-38.195-12.412-50.666-19.525c-8.225-4.691-12.669-8.658-14.739-11.029C36.489,80.963,40.933,76.996,49.158,72.304z"/>
                            </svg>
                            Фильтровать
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>